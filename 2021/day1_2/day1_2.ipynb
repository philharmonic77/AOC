   "source": [
    "with open('./input.txt', 'r') as f :\n",
    "    lines = f.readlines() \n",
    "    \n",
    "    file_size = len(lines) \n",
    "    \n",
    "    counter = 0\n",
    "    for i in range(file_size - 3):\n",
    "        if int(lines[i+3]) > int(lines[i]):\n",
    "            counter += 1 \n",
    "print(counter)"
   ]
